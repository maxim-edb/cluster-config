function(username){
  kind: 'Route',
  apiVersion: 'route.openshift.io/v1',
  metadata: {
    name: 'grafana',
    namespace: username + '-tools',
    annotations: {
      'argocd.argoproj.io/sync-wave': '5',
    },
  },
  spec: {
    to: {
      kind: 'Service',
      name: 'grafana-' + username + '-tools-service',
      weight: 100,
    },
    port: {
      targetPort: 'grafana',
    },
    tls: {
      termination: 'edge',
    },
    wildcardPolicy: 'None',
  },
}

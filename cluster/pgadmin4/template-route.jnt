function(username) {
  kind: 'Route',
  apiVersion: 'route.openshift.io/v1',
  metadata: {
    name: 'pgadmin',
    namespace: username + '-tools',
  },
  spec: {
    to: {
      kind: 'Service',
      name: 'pgadmin4-service',
      weight: 100,
    },
    port: {
      targetPort: 80,
    },
    wildcardPolicy: 'None',
  },
}

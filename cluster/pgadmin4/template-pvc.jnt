function(username) {
  apiVersion: 'v1',
  kind: 'PersistentVolumeClaim',
  metadata: {
    name: 'pgadmin4-' + username + '-tools-pvc',
    namespace: username + '-tools',
  },
  spec: {
    accessModes: [
      'ReadWriteOnce',
    ],
    resources: {
      requests: {
        storage: '1Gi',
      },
    },
  },
}

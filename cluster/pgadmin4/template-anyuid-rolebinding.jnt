function(username) {
  apiVersion: 'rbac.authorization.k8s.io/v1',
  kind: 'RoleBinding',
  metadata: {
    name: 'anyuid-default',
    namespace: username + '-tools',
  },
  roleRef: {
    apiGroup: 'rbac.authorization.k8s.io',
    kind: 'ClusterRole',
    name: 'system:openshift:scc:anyuid',
  },
  subjects: [
    {
      kind: 'ServiceAccount',
      name: 'default',
      namespace: username + '-tools',
    },
  ],
}
